{
  "name": "webpack-webassembly-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.tsx",
  "scripts": {
    "start": "webpack serve",
    "test": "jest",
    "build": "webpack --mode production",
    "build:wasm": "cd ./public && emcc ../src/webAssembly/main.c -s WASM=1 -o main.wasm --no-entry && cd ../",
    "lint": "eslint src/",
    "prettier": "prettier --write src",
    "prettier-watch": "onchange \"**/*\" -- prettier --write src",
    "cm": "git-cz",
    "doc": "typedoc --out docs/ --readme ./README.md src/index.tsx",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "author": "",
  "license": "ISC",
  "babel": {
    "presets": [
      "@babel/react",
      "@babel/preset-env",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.168",
    "@types/react-dom": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "babel-loader": "^8.2.2",
    "before-build-webpack": "^0.2.11",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^5.2.4",
    "cz-conventional-changelog": "^3.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "extra-watch-webpack-plugin": "^1.0.3",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^27.0.3",
    "onchange": "^7.1.0",
    "prettier": "^2.3.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.1.2",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4",
    "url": "^0.11.0",
    "url-loader": "^4.1.1",
    "webassembly-loader": "^1.1.0",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  }
}
